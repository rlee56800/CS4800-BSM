public class Cart
{
    //all the customer class related information is commented out because it depends on a database that doesn't exist.
    private Order order;
    //private Customer customer;
	  
	  public Cart()
	  {
	    order = new Order(0, null);
	   // customer = new Customer();
	  }
	  
	  public Cart(Order o)//, Customer c)
	  {
	    order = o;
	    //customer = c;
	  }
	  
	  
	  //getters and setters
	  public void setOrder(Order o)
	  {
	    order = o;
	  }
	  
      /*
	  public void setCustomer(Customer c)
	  {
	    customer = c;
	  }
      */
	  
	  public Order getOrder()
	  {
	    return order;
	  }
	  
      /*
	  public Customer getCustomer()
	  {
	    return customer;
	  }
	  */
  
}//end class
