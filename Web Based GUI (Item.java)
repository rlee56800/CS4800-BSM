import java.util.ArrayList;

public class Item {
	//attributes
	private String itemID;
	private int price;
	private String name;
	private boolean studentcheck;
 
  
	//constructor
	public Item()
	{
		this.itemID="";
        this.price=0;
        this.name="";
	}

    public Item(String itemID, int price, String name)
    {
        this.itemID = itemID;
        this.price = price;
        this.name = name;
    }
  


    public void setItemID(String i)
    {
        this.itemID = i;
    }

	public void setPrice(int p)
	{
		this.price = p;
	}
  

	 
	public void setName(String n)
	{
		this.name = n;
	}
	
	public void setStudent(boolean s) 
	{
		this.studentcheck = s;
	}
  
	
	public String getItemID()
	{
		return this.itemID;
	}
	
	

	public int getPrice()
	{
		return this.price;
	}
  

	public String getName()
	{
        return this.name;
    }
  
  
	public boolean getStudent() 
	{
		return this.studentcheck;
	}
}
